General: 
  ControllerName: 
    GoogleUploadController
  HandlerParentPath: 
    /upload/google

Function:
  Select Ad Type: 
    get /adTypeSelection
    
  Create Banner Ad: 
    get /bannerAd/create
    post /bannerAd/confirm
    post /bannerAd/complete
    
  Create Banner&Text Ad:
    get /bannerTextAd/create
    post /bannerTextAd/confirm
    post /bannerTextAd/complete
    
  Create Text Ad:
    get /textAd/create
    post /textAd/confirm
    post /textAd/complete